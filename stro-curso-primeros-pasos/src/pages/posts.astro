---
let messageError;
let data;
try {
    const response = await fetch("https://jsonplaceholder.typicode.com/posts");
    data = await response.JSON();
    console.log(data);
} catch (error) {
    messageError = "No se logro establecer la coneccion con la api";
}
---

<h3>Mis articulos</h3>
{
    !messageError ? (
        data.map((post) => <div>post.body</div>)
    ) : (
        <p class="title-danger">{messageError}</p>
    )
}
